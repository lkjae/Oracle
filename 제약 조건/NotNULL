#테이블 생성 제약
CREATE TABLE TABLE_NOTNULL(
    LOGIN_ID    VARCHAR2(20) NOT NULL,
    LOGIN_PWD   VARCHAR2(20) NOT NULL,
    TEL         VARCHAR2(20)
);


#14-3제약 조건이 없는 TEL열에 NULL값 입력
INSERT INTO TABLE_NOTNULL (LOGIN_ID, LOGIN_PWD)
VALUES ( 'TEST_ID_01','1234');

#14-5제약 조건 살펴보기
SELECT OWNER, CONSTRAINT_NAME, CONSTRAINT_TYPE, TABLE_NAME
FROM USER_CONSTRAINTS;


#14-7 제약조건 추가
SELECT * FROM TABLE_NOTNULL;

ALTER TABLE TABLE_NOTNULL
MODIFY (TEL NOT NULL);

#14-8 TEL 컬럼 NULL제거 ->UPDATE
UPDATE TABLE_NOTNULL
   SET TEL = '010-1111-1111'
   WHERE LOGIN_ID = 'TEST_ID_01';


#14-9 NOT NULL 제약 조건 추가하기
ALTER TABLE TABLE_NOTNULL
MODIFY (TEL NOT NULL);

#14-12 이미 생성된 제약 조건 이름 변경하기
ALTER TABLE TABLE_NOTNULL2
RENAME CONSTRAINT TBLN_TEL_NN TO TBLN2_TEL_NN;

ALTER TABLE TABLE_NOTNULL2
MODIFY (TEL NOT NULL);      (이름을 바꿔주는 과정을 생략하려면 
	MODIFY(TEL CONSTRAINT MY_01 NOTNULL)을 제약조건을 추가하는 부분에서 이름 설정)














